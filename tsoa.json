{
  "spec": {
    "name": "IODC",
    "description": "IODC Provider",
    "version": "1.0.0",
    "schemes": [
      "http",
      "https"
    ],
    "outputDirectory": "./",
    "specFileBaseName": "openapi",
    "specVersion": 3,
    "yaml": true,
    "host": "localhost:8080"
  },
  "noImplicitAdditionalProperties": "throw-on-extras",
  "entryFile": "./src/config/app.ts",
  "controllerPathGlobs": [
    "src/controllers/*controller.ts"
  ],
  "routes": {
    "routesDir": "./src/routes",
    "routesFileName": "routes.ts",
    "useSuccessResponseCode": "false"
  }
}